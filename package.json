{
  "name": "juglans",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "bench": "make -C benchmarks",
    "lint": "eslint --ext .js lib benchmarks",
    "build": "babel lib --out-dir dist --copy-files",
