{
    "name": "Juglans V1.0",
    "prefix": "/api/v1",
    "port": 3000,
    "debug": true,
    "logger": {
        "service": "Juglans V1.0",
        "maxsize": 10240,
        "path": ""
    },
    "bodyParser": {
        "strict": false,
        "jsonLimit": "5mb",
        "formLimit": "1mb",
        "textLimit": "1mb",
        "multipart": true,
        "formidable": {
            "keepExtensions": true
        }
    }
}